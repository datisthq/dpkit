{
  "name": "@dpkit/terminal",
  "type": "module",
  "version": "0.0.0-dev",
  "private": true,
  "scripts": {
    "build": "tsc && pnpm build:copy && pnpm build:mode",
    "build:copy": "cp entrypoints/*.cmd build/entrypoints && cp package.json build",
    "build:mode": "chmod +x ./build/entrypoints/*.js",
    "compile": "node @compile.ts",
    "dev": "node ./entrypoints/dev.ts",
    "run": "node ./entrypoints/run.ts"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@commander-js/extra-typings": "^14.0.0",
    "frictionless-ts": "1.0.1",
    "commander": "^14.0.0",
    "es-toolkit": "^1.39.10",
    "exit-hook": "^4.0.0",
    "ink": "^6.3.1",
    "nodejs-polars": "^0.22.2",
    "picocolors": "^1.1.1",
    "react": "^19.1.1",
    "react-devtools-core": "^6.1.2",
    "tiny-invariant": "^1.3.3",
    "ts-extras": "^0.14.0"
  },
  "devDependencies": {
    "@types/node": "24.2.0",
    "@types/react": "19.1.9",
    "ink-testing-library": "4.0.0"
  }
}
