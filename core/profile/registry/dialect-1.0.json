{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Table Dialect",
  "properties": {
    "csvddfVersion": {
      "title": "CSV Dialect schema version",
      "type": "number",
      "default": 1.2
    },
    "delimiter": {
      "title": "Delimiter",
      "type": "string",
      "default": ","
    },
    "doubleQuote": {
      "title": "Double Quote",
      "type": "boolean",
      "default": true
    },
    "lineTerminator": {
      "title": "Line Terminator",
      "type": "string",
      "default": "\r\n"
    },
    "nullSequence": {
      "title": "Null Sequence",
      "type": "string"
    },
    "quoteChar": {
      "title": "Quote Character",
      "type": "string",
      "default": "\""
    },
    "escapeChar": {
      "title": "Escape Character",
      "type": "string"
    },
    "skipInitialSpace": {
      "title": "Skip Initial Space",
      "type": "boolean",
      "default": false
    },
    "header": {
      "title": "Header",
      "type": "boolean",
      "default": true
    },
    "commentChar": {
      "title": "Comment Character",
      "type": "string"
    },
    "caseSensitiveHeader": {
      "title": "Case Sensitive Header",
      "type": "boolean",
      "default": false
    }
  }
}
