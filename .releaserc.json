{
  "branches": ["main"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "pnpm ci:version -- ${nextRelease.version}",
        "publishCmd": "pnpm ci:publish"
      }
    ],
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "pnpm compile"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": "dpkit/compile/build/*.zip"
      }
    ]
  ]
}
