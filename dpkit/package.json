{
  "name": "dpkit",
  "type": "module",
  "version": "0.0.0-dev",
  "exports": "./build/index.js",
  "sideEffects": false,
  "files": ["build"],
  "bin": {
    "dpkit": "./build/entrypoints/run.js"
  },
  "license": "MIT",
  "author": "Evgeny Karev",
  "repository": "https://github.com/datisthq/dpkit",
  "description": "Fast TypeScript data management framework built on top of the Data Package standard and Polars DataFrames",
  "keywords": [
    "data",
    "polars",
    "dataframe",
    "datapackage",
    "tableschema",
    "typescript",
    "validation",
    "quality",
    "fair",
    "library",
    "terminal"
  ],
  "scripts": {
    "build": "tsc && pnpm build:copy && pnpm build:mode",
    "build:copy": "cp entrypoints/*.cmd build/entrypoints && cp package.json build",
    "build:mode": "chmod +x ./build/entrypoints/*.js"
  },
  "dependencies": {
    "@dpkit/library": "workspace:*",
    "@dpkit/terminal": "workspace:*"
  }
}
